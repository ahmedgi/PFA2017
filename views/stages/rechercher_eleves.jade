extends layout
  
block content
  form.form-horizontal#fichier_eleves_form(action="/stages/eleves/trouver", method="post")
    .form-group
      label.col-sm-2(for="annee_universitaire") Année universitaire
      .col-sm-10
        input.form-control#annee_universitaire(name="annee_universitaire")
    .form-group
      label.col-sm-2(for="filiere") Filière
      .col-sm-10
        input.form-control#filiere(name="filiere")
    .form-group
      label.col-sm-2(for="niveau") Niveau
      .col-sm-10
        input.form-control#niveau(name="niveau")
    button.btn.btn-primary(type="submit") Trouver
